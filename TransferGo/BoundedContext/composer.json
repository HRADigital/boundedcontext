{
    "name": "transfergo/boundedcontext",
    "description": "My Bounded Context.",
    "keywords": [
        "TransferGo",
        "bounded",
        "context"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.0",
        "symfony/string": "^5.4",
        "cakephp/utility": "^4.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "classmap": [
            "TransferGo/BoundedContext/"
        ],
        "psr-4": {
            "TransferGo\\BoundedContext\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TransferGo\\Tests\\BoundedContext\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
